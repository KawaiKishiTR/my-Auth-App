<Ayarlar>:
    name: "ayarlar"
    BoxLayout:
        id: ana_kutu
        orientation: "vertical"
        padding: 20
        spacing: 10
        Button:
            text: "AnaEkran'a Git"
            on_release:
                app.root.current = "ana"


        Label:
            id: tema_label
            text: "Geçerli tema: default"

        Spinner:
            id: tema_spinner
            text: "Tema Seç"
            values: root._get_theme_list()
            on_text: root.tema_degistir(self.text)
